<view class="search-item" bind:tap="searchSongItemClickHandle">
  <view class="order">{{index}}</view>
  <view class="content">
    <view class="name">{{item.name}}</view>
    <view class="author">
      <image class="image" src="/assets/image/icons/sq_icon.png" mode="widthFix"></image>
      <text class="text">{{item.artists[0].name}}</text>
    </view>
  </view>
</view>